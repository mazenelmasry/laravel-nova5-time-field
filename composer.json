{
    "name": "mazenelmasry/laravel-nova5-time-field",
    "description": "Custom HTML5 time field for Laravel Nova 5.",
    "keywords": [
        "laravel",
        "nova",
        "time"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "MazenElmasry\\TimeField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MazenElmasry\\TimeField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MazenElmasry\\TimeField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
