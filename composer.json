{
    "name": "michielfb/laravel-nova-time-field",
    "description": "A HTML5 time field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Michielfb\\Time\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Michielfb\\Time\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michielfb\\Time\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
